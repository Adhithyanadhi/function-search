{
  "name": "function-name-search",
  "displayName": "Function Name Search",
  "publisher": "AmbitiousCoder",
  "description": "Search & Navigate functions instantly",
  "repository": {
    "url": "https://github.com/Adhithyanadhi/function-search"
  },
  "icon": "icons/icon.png",
  "version": "0.3.3",
  "engines": {
    "vscode": "^1.22.2"
  },
  "categories": [
    "Programming Languages",
    "Other",
    "Language Packs"
  ],
  "keywords": [
    "function search",
    "function name search",
    "navigate",
    "code navigation",
    "go to definition",
    "go to function",
    "go to function definition",
    "symbol search",
    "fast search",
    "refactor",
    "index functions"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.searchFunction",
        "title": "Search Function Name"
      }
    ],
    "keybindings": [
      {
        "command": "extension.searchFunction",
        "key": "ctrl+k",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "eslint": "^9.23.0"
  },
  "dependencies": {
    "vsce": "^2.15.0"
  }
}