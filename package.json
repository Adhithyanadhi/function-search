{
  "name": "function-name-search",
  "displayName": "Function Name Search",
  "publisher": "AmbitiousCoder",
  "description": "Search & Navigate functions instantly",
  "repository": {
    "url": "https://github.com/Adhithyanadhi/function-search"
  },
  "icon": "icons/icon.png",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.22.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.searchFunction",
        "title": "Search Function Name"
      }
    ],
    "keybindings": [
      {
        "command": "extension.searchFunction",
        "key": "ctrl+k",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.22.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0"
  },
  "dependencies": {
    "vsce": "^2.15.0"
  }
}
