{
  "name": "function-name-search",
  "displayName": "Function Name Search",
  "publisher": "AmbitiousCoder",
  "version": "0.5.0",
  "description": "Search & Navigate functions instantly",
  "license": "MIT",
  "engines": { "vscode": "^1.80.0" },
  "activationEvents": [
    "onStartupFinished",
    "onCommand:function-name-search.searchFunction",
    "onCommand:function-name-search.clearIndex",
    "onView:function-name-search"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      { "command": "function-name-search.searchFunction", "title": "Function Name Search: Search Function" },
      { "command": "function-name-search.clearIndex", "title": "Function Name Search: Clear Index" }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "test": "echo \"(no tests)\"",
    "package": "vsce package"
  },
  "dependencies": {
    "node-sqlite3-wasm": "0.8.51"
  },
  "devDependencies": {
    "@vscode/vsce": "^3.0.0",
    "@types/node": "20.x",
    "eslint": "^9.23.0"
  }
}
